{
  "urn": "urn:mediator:climate-mediator",
  "version": "0.0.1",
  "name": "Climate Mediator",
  "description": "Processes climate-related data as an example of unstructured data handling",
  "defaultChannelConfig": [
    {
      "name": "Climate Mediator",
      "urlPattern": "^/climate.*$",
      "routes": [
        {
          "name": "Climate Mediator Endpoint",
          "host": "172.17.0.1",
          "port": "3000",
          "primary": true,
          "type": "http"
        }
      ],
      "allow": [
        "instant"
      ],
      "methods": [
        "POST", "GET"
      ],
      "type": "http"
    }
  ],
  "endpoints": [
    {
      "name": "Climate Endpoint",
      "host": "172.17.0.1",
      "path": "/test",
      "port": "3000",
      "primary": true,
      "type": "http"
    }
  ],
  "configDefs": [{
    "param": "minio_buckets_registry",
      "displayName": "Minio Buckets Registry",
      "description": "The available Minio buckets and their configurations",
      "type": "struct",
      "array": true,
      "template":[{
        "param": "bucket",
        "type": "string"
      }]
  }]
}
